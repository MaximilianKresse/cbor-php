{
    "name": "spomky-labs/cbor-php",
    "type": "library",
    "license": "MIT",
    "description": "CBOR Encoder/Decoder for PHP",
    "authors": [
        {
            "name": "Spomky"
        }
    ],
    "autoload": {
        "psr-4": {
            "CBOR\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CBOR\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1",
        "spomky-labs/base64url": "^1.0",
        "ext-gmp": "*",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "infection/infection": "^0.8.2",
        "phpunit/phpunit": "^7.0",
        "satooshi/php-coveralls": "^2.0",
        "phpstan/phpstan": "^0.9.2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
